    "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME": "dzwkeydij",
    "CLOUDINARY_URL": "cloudinary://261241242864329:KS0GJUBWc5m5gyMXLC2yPPozVuA@dzwkeydij",
    "NEXT_PUBLIC_CLOUDINARY_UPLOAD_PRESET": "airbnb_upload",
