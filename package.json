{
  "name": "ewaahome",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-warnings' next dev -p 3008",
    "alt-dev": "cross-env NODE_OPTIONS='--no-warnings' next dev -p 3009",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint:setup": "node scripts/setup-eslint.js",
    "postinstall": "node -e \"try { require('./prisma/generate-client.js') } catch(e) { console.log('Falling back to direct generation'); try { require('@prisma/client'); } catch(e) { console.log('Continuing anyway'); } }\"",
    "prepare-vercel": "node scripts/prepare-vercel.js",
    "vercel-build": "npm run prepare-vercel && node scripts/vercel-deploy.js && node scripts/vercel-build-info.js && npx prisma generate && next build",
    "netlify-build": "prisma generate && npm run build",
    "db:test": "node scripts/test-db.js",
    "db:mongo-test": "node scripts/mongo-test.js",
    "prisma:studio": "prisma studio",
    "app:verify": "node scripts/verify-app.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.6.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.4",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^2.15.0",
    "mongodb": "^6.15.0",
    "next": "^14.0.4",
    "next-auth": "^4.24.5",
    "query-string": "^9.1.1",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.0.1",
    "react-leaflet": "^4.2.1",
    "react-select": "^5.7.4",
    "react-spinners": "^0.13.8",
    "react-toastify": "^10.0.4",
    "swr": "^2.2.5",
    "world-countries": "^4.0.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.40.0",
    "@types/bcrypt": "^5.0.2",
    "@types/leaflet": "^1.9.8",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-date-range": "^1.4.6",
    "@types/react-dom": "^18.2.19",
    "@types/react-leaflet": "^2.8.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-react": "^7.34.1",
    "glob": "^10.3.10",
    "postcss": "^8.4.35",
    "prisma": "^5.22.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma"
  }
}
